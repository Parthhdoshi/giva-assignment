{
  "extends": "next/core-web-vitals",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "indent": ["error", 2],
    // "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": "error",
    "no-unused-labels" :  "error",
    "no-console": "warn",
    // "no-magic-numbers" : "error",
    "no-mixed-operators":"error"
    // "no-var":"error"
  }
}